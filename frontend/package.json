    "@novu/node": "^2.0.0-canary.1",
    "@novu/notification-center": "^2.0.0-canary.0",
    "@react-pdf/renderer": "^3.4.4",
    "@sentry/react": "^7.99.0",
    "@sentry/vite-plugin": "^2.13.0",
    "@tiptap/extension-link": "2.0.0-beta.217",
    "@tiptap/extension-text-align": "2.0.0-beta.217",
    "@tiptap/pm": "2.0.0-beta.217",
    "@tiptap/react": "2.0.0-beta.217",
    "@tiptap/starter-kit": "2.0.0-beta.217",
    "axios": "1.3.2",
    "date-fns": "^3.6.0",
    "html2canvas": "^1.4.1",
    "i18next": "22.4.9",
    "keycloak-js": "^23.0.3",
    "nanoid": "4.0.1",
    "prosemirror-commands": "1.5.0",
    "prosemirror-dropcursor": "1.7.0",
    "prosemirror-gapcursor": "1.3.1",
    "prosemirror-history": "1.3.0",
    "prosemirror-keymap": "1.2.0",
    "prosemirror-schema-list": "1.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "7.43.1",
    "react-i18next": "12.1.5",
    "react-icons": "^5.2.1",
    "react-router-dom": "6.8.1",
    "react-toastify": "9.1.1",
    "react-transition-group": "4.4.5",
    "recharts": "2.4.1"
